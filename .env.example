# STAC COPC Catalog - Environment Configuration
# Copy this file to .env and fill in your values

# AWS Configuration
AWS_REGION=ap-northeast-1
STAC_BUCKET_NAME=stac-copc-catalog

# CloudFront / Domain Configuration
STAC_DOMAIN=stac.example.com
ACM_CERTIFICATE_ARN=arn:aws:acm:us-east-1:123456789012:certificate/xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx

# STAC Catalog Configuration
STAC_CATALOG_ID=stac-copc-catalog
STAC_COLLECTION_ID=pointcloud-jgd2011
STAC_BASE_URL=https://stac.example.com

# STAC Browser Configuration
STAC_BROWSER_VERSION=3.2.0

# Local Paths (optional, defaults to ./local/*)
# INPUT_DIR=./local/input
# OUTPUT_DIR=./local/output
# CATALOG_DIR=./catalog
